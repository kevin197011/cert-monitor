# Nacos configuration center connection info
NACOS_ADDR=http://localhost:8848
NACOS_NAMESPACE=devops
NACOS_GROUP=DEFAULT_GROUP
NACOS_DATA_ID=cert_domains

# Application configuration
PORT=9393
LOG_LEVEL=info

# SSL certificate check configuration
CHECK_INTERVAL=60  # Check interval in seconds (default: 60)
CONNECT_TIMEOUT=10  # Connection timeout in seconds
EXPIRE_WARNING_DAYS=30  # Certificate expiration warning threshold in days
MAX_CONCURRENT_CHECKS=50  # Maximum number of concurrent domain checks (default: 50)

# Nacos polling configuration
NACOS_POLL_INTERVAL=30  # Nacos configuration polling interval in seconds (default: 30)