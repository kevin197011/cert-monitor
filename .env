# Nacos配置中心连接信息
NACOS_ADDR=http://192.168.1.19:8848
NACOS_NAMESPACE=devops
NACOS_GROUP=DEFAULT_GROUP
NACOS_DATA_ID=cert-monitor

# 应用配置
PORT=9393
LOG_LEVEL=info

# SSL证书检测配置
CHECK_INTERVAL=60  # 检测间隔（秒）
CONNECT_TIMEOUT=10  # 连接超时（秒）

# 监控告警阈值
EXPIRE_WARNING_DAYS=30  # 证书过期预警天数

# Nacos polling configuration
NACOS_POLL_INTERVAL=30  # Nacos configuration polling interval in seconds (default: 30)

# Maximum number of concurrent domain checks
MAX_CONCURRENT_CHECKS=50  # Maximum number of concurrent domain checks (default: 50)